#window {
	border: 2px solid #54964f;
	border-radius: 4px;
	background-color: #161616;

	font-size: 16px;
	font-family: FiraCode Nerd Font Mono
}

#outer-box {
	margin: 6px;
	background-color: #161616;
}

#inner-box {
	margin: 0px;
	background-color: #161616;
}
#scroll {
	margin: 0px;
}

#input {
	margin: 0px;
	background-color: #161616;

	border: none;
	padding-bottom: 4px;
	margin-bottom: 4px;
	border-bottom: 2px dashed #e0e0e0;
	border-radius: 0px;
}

#text {
	margin: 0px;
	border: none;
	padding: 6px;
}
#text:selected {
}


#entry {
}
#entry:selected {
	outline: none;
	background-color: #54964f;
}

#img {
	padding-left: 10px;
	padding-right: 20px;
}

